repeat wait() until game:isLoaded()

RunService = game:GetService("RunService")
local NotificationHolder = loadstring(game:HttpGet("https://raw.githubusercontent.com/BocusLuke/UI/main/STX/Module.Lua"))()
local Notification = loadstring(game:HttpGet("https://raw.githubusercontent.com/BocusLuke/UI/main/STX/Client.Lua"))()
function distanceTo(CF)
    local player = game.Players.LocalPlayer
    if player.Character and player.Character:FindFirstChild("HumanoidRootPart") and CF and CF.Position then
        local playerPosition = player.Character.HumanoidRootPart.Position
        local targetPosition = CF.Position

        local distance = math.floor((playerPosition - targetPosition).Magnitude / 10)
        return distance
    else
        return 10
    end
end

function getRoot(char)
   local rootPart = char:FindFirstChild('HumanoidRootPart') or char:FindFirstChild('Torso') or char:FindFirstChild('UpperTorso')
   return rootPart
end
function float()
   local pchar = game:GetService("Players").LocalPlayer.Character
   if pchar and not pchar:FindFirstChild("AetherFloat") then
      task.spawn(function()
         local Float = Instance.new('Part')
         Float.Name = "AetherFloat"
         Float.Parent = pchar
         Float.Transparency = 0.8
         Float.Size = Vector3.new(15,0.2,15)
         Float.Anchored = true
         local FloatValue = -3.9
         Float.CFrame = getRoot(pchar).CFrame * CFrame.new(0,FloatValue,0)
         floatDied = game.Players.LocalPlayer.Character:FindFirstChildOfClass('Humanoid').Died:Connect(function()
            FloatingFunc:Disconnect()
            Float:Destroy()
         end)
         local function FloatPadLoop()
            if pchar:FindFirstChild("AetherFloat") and getRoot(pchar) then
               Float.CFrame = getRoot(pchar).CFrame * CFrame.new(0,FloatValue,0)
            else
               FloatingFunc:Disconnect()
               Float:Destroy()
               floatDied:Disconnect()
            end
         end         
         FloatingFunc = RunService.Heartbeat:Connect(FloatPadLoop)
      end)
   end
end
function unfloat()
   local pchar = game:GetService("Players").LocalPlayer.Character
   if pchar:FindFirstChild("AetherFloat") then
      pchar:FindFirstChild("AetherFloat"):Destroy()
   end
   if floatDied then
      FloatingFunc:Disconnect()
      floatDied:Disconnect()
   end
end

function tween(to)
   float()
   local player = game.Players.LocalPlayer
   local TweenService = game:GetService("TweenService")
   local time = distanceTo(to) / 20
   if to and getRoot(player.Character) then
    TweenService:Create(getRoot(player.Character), TweenInfo.new(time, Enum.EasingStyle.Linear), {CFrame = to}):Play()
   end
   wait(time)
   unfloat()
end
function addStats(id)
    local places = {
        "Melee",
        "Defense",
        "Sword",
        "Gun",
        "Devil Fruit"
    }
    if not places[id] then return end
    local args = {
        [1] = "AddPoint",
        [2] = places[id],
        [3] = getgenv().statsQuantidade
    }
    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
end

function EquipWeapon(ToolSe)
	if game.Players.LocalPlayer.Backpack:FindFirstChild(ToolSe) then
        local tool = game.Players.LocalPlayer.Backpack:FindFirstChild(ToolSe)
        wait(.2)
        game.Players.LocalPlayer.Character.Humanoid:EquipTool(tool)
	end 
end

local circleRadius = 10
local rotationSpeed = 2
--[[
function spin(cf)
    local currentTime = tick()
    local angle = currentTime * rotationSpeed
    local xOffset = math.cos(angle) * circleRadius
    local zOffset = math.sin(angle) * circleRadius

    local targetPosition = cf.Position + Vector3.new(xOffset, 0, zOffset)
    local lookVector = (cf.Position - targetPosition).unit

    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(targetPosition) * CFrame.Angles(0, math.atan2(lookVector.X, lookVector.Z), 0)
end
]]--

function spin(cf)
    local cframes = {
        cf - Vector3.new(10, 0, 10),
        cf - Vector3.new(10, 0, -10),
        cf - Vector3.new(-10, 0, 10),
        cf - Vector3.new(0, 0, 10),
        cf - Vector3.new(10, 0, 0),
        cf - Vector3.new(0, 0, -10),
        cf - Vector3.new(-10, 0, 0),
        cf - Vector3.new(20, 0, 20),
        cf - Vector3.new(20, 0, -20),
        cf - Vector3.new(-20, 0, 20),
        cf - Vector3.new(0, 0, 20),
        cf - Vector3.new(20, 0, 0),
        cf - Vector3.new(0, 0, -20),
        cf - Vector3.new(-20, 0, 0),
        cf - Vector3.new(15, 0, 15),
        cf - Vector3.new(15, 0, -15),
        cf - Vector3.new(-15, 0, 15),
        cf - Vector3.new(0, 0, 15),
        cf - Vector3.new(15, 0, 0),
        cf - Vector3.new(0, 0, -15),
        cf - Vector3.new(-15, 0, 0),
    }

    local index = math.random(1, #cframes)
    local cframe = cframes[index]
    if distanceTo(cf) < 30 then
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = cframe
    end
end
function enableBuso()
    local args = {
        [1] = "Buso"
    }
    game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("CommF_"):InvokeServer(unpack(args))
end
function spawnSeaBeast()
    local vim = game:GetService('VirtualInputManager')

    local SafePort = CFrame.new(2234.01318, 5.36201763, -6255.82031, -0.817939579, 0, 0.575304091, 0, 1, 0, -0.575304091, 0, -0.817939579)

    local function pressKey(key)
        vim:SendKeyEvent(true, key, false, game)
    end
    local function releaseKey(key)
        vim:SendKeyEvent(false, key, false, game)
    end
    EnemyExitCode = 0
    function noEnemy()
        local safe = true
        for _, v in ipairs(game:GetService("Workspace"):FindFirstChild("Boats"):GetChildren()) do
            if v and not v:FindFirstChild("Owner") and distanceTo(v:FindFirstChild("VehicleSeat").CFrame) < 100 then
                safe = false
                EnemyExitCode = 1 -- Pirate Raid
            end
        end
        for i,v in pairs(game:GetService("Workspace").SeaBeasts:GetChildren()) do
            if v:FindFirstChild("HumanoidRootPart") and distanceTo(v:FindFirstChild("HumanoidRootPart").CFrame) < 100 then
                safe = false 
                EnemyExitCode = 2 -- Sea Beast
            end
        end

        print(EnemyExitCode)
        print(safe)
        return safe
    end

    function playerInSeat()
        return game.Players.LocalPlayer.Character.Humanoid.Sit
    end
    if distanceTo(SafePort) > 100 then
        float()
        tween(SafePort)
        unfloat()
    end
    local args = {
        [1] = "BuyBoat",
        [2] = "MarineBasic"
    }
    game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("CommF_"):InvokeServer(unpack(args))
    wait(2)
    for _, v in ipairs(game:GetService("Workspace"):FindFirstChild("Boats"):GetChildren()) do
        if v and v.Name == "MarineBasic" and v:FindFirstChild("VehicleSeat") and v:FindFirstChild("Owner") and tostring(v:FindFirstChild("Owner").Value) == game.Players.LocalPlayer.Name then
            v:FindFirstChild("VehicleSeat").MaxSpeed = 300
            tween(v:FindFirstChild("VehicleSeat").CFrame)
            wait(1)

            while noEnemy() and playerInSeat() do
                pressKey(Enum.KeyCode.W)
                wait()
            end
            releaseKey(Enum.KeyCode.W)
            wait(0.1)
            pressKey(Enum.KeyCode.S)
            wait(2)
            releaseKey(Enum.KeyCode.S)
            pressKey(Enum.KeyCode.Space)
            wait(0.1)
            releaseKey(Enum.KeyCode.Space)
            wait(0.1)
        end
    end
    wait(1)
end
local placeId = game.placeId
if placeId == 2753915549 then FirstSea = true
elseif placeId == 4442272183 then SecondSea = true
elseif placeId == 7449423635 then ThirdSea = true end


