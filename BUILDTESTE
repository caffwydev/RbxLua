spawn(function()
    while true do
        GetQuest()
        wait(0.1)
    end
end)
AutoFarmEnabled = false
function GoForQuest()
    GetQuest()
    local args = {
        [1] = "StartQuest",
        [2] = QuestName,
        [3] = QuestId
    }
    tween(AdventurerLocation)
    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
end

function AutoFarm()

    if (AutoFarmEnabled) then
        float()
    else
        unfloat()
    end

    spawn(function() pcall(function()
        while AutoFarmEnabled do
            wait()
            if distanceTo(IdleLocation) < 50 and game:GetService("Players").LocalPlayer.PlayerGui:FindFirstChild("Main"):FindFirstChild("Quest").Visible then
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = IdleLocation
            elseif distanceTo(IdleLocation) > 5000 then
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = IdleLocation
            else
                tween(IdleLocation)
            end
            for _, v in ipairs(game:GetService("Workspace").Enemies:GetChildren()) do
                if v.Name == NPCName and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then
                        local QuestContainer = game:GetService("Players").LocalPlayer.PlayerGui:FindFirstChild("Main"):FindFirstChild("Quest")
                        if not QuestContainer.Visible then
                            GoForQuest()
                        elseif not string.find(string.upper(QuestContainer.Container.QuestTitle.Title.Text), string.upper(FriendlyName)) then
                            GoForQuest()
                        end
                        if distanceTo(v.HumanoidRootPart.CFrame) > 75 then
                            tween(v.HumanoidRootPart.CFrame + Vector3.new(0, 10, 0))
                        else
                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame + Vector3.new(0, 10, 0)
                        end
                        Attacking = true
                        spawn(function() pcall(function()
                            while (v.Humanoid.Health > 1) and AutoFarmEnabled do
                                spin(v.HumanoidRootPart.CFrame)
                                wait(0.02)
                            end
                        end)
                        end)
                        while (v.Humanoid.Health > 1) and AutoFarmEnabled do
                            v.HumanoidRootPart.Size = Vector3.new(50, 50, 50)
                            v.Humanoid.WalkSpeed = 0
                            v.Humanoid.JumpPower = 0
                            sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius", math.huge) 
                            wait()
                        end
                        Attacking = false
            end
        end
    end
    end)
end)
end

local function compareStrings(a, b)
    return a < b
end



spawn(function() pcall(function()
    while true do
        wait()
        spawn(function()
            if not game.Players.LocalPlayer.Character:FindFirstChild("HasBuso") then
                enableBuso()
            end
        end)
    end
end)
end)































local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()


local Window = OrionLib:MakeWindow({Name = "Aether Build de Teste BT1_21_08_23",
    HidePremium = false,
    SaveConfig = true
})



local Tab = Window:MakeTab({
	Name = "Teste",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})


local Section = Tab:AddSection({
	Name = "Teste"
})
local WeaponList = {""}
WeaponSelect = false

local Dropdown = Section:AddDropdown({
	Name = "Armas",
	Default = "",
	Options = {""},
	Callback = function(Value)
		print(Value)
	end    
})


function reWeapon()
        table.clear(WeaponList)
    	for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
    		if v:IsA("Tool") then
    			table.insert(WeaponList,v.Name)
    		end
    	end
    	for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
    		if v:IsA("Tool") then
                table.insert(WeaponList,v.Name)
    		end
    	end
        table.sort(WeaponList, compareStrings)
        Dropdown:Refresh(WeaponList, true)
    end

Tab:AddButton({
	Name = "Armas!",
	Callback = function()
      	reWeapon()
  	end    
})

Tab:AddToggle({
	Name = "Farm!",
	Default = false,
	Callback = function(Value)
		AutoFarmEnabled = Value
        AutoFarm()
	end    
})

OrionLib:Init()

































































pcall(function()
    local plr = game.Players.LocalPlayer
    local CbFw = getupvalues(require(plr.PlayerScripts.CombatFramework))
    local CbFw2 = CbFw[2]
    function GetCurrentBlade() 
            local p13 = CbFw2.activeController
            local ret = p13.blades[1]
            if not ret then return end
            while ret.Parent~=game.Players.LocalPlayer.Character do ret=ret.Parent end
            return ret
        end
    function AttackNoCD()
                local AC = CbFw2.activeController
                for i = 1, 1 do 
                    local bladehit = require(game.ReplicatedStorage.CombatFramework.RigLib).getBladeHits(
                        plr.Character,
                        {plr.Character.HumanoidRootPart},
                        60
                    )
                    local cac = {}
                    local hash = {}
                    for k, v in pairs(bladehit) do
                        if v.Parent:FindFirstChild("HumanoidRootPart") and not hash[v.Parent] then
                            table.insert(cac, v.Parent.HumanoidRootPart)
                            hash[v.Parent] = true
                        end
                    end
                    bladehit = cac
                    if #bladehit > 0 then
                        local u8 = debug.getupvalue(AC.attack, 5)
                        local u9 = debug.getupvalue(AC.attack, 6)
                        local u7 = debug.getupvalue(AC.attack, 4)
                        local u10 = debug.getupvalue(AC.attack, 7)
                        local u12 = (u8 * 798405 + u7 * 727595) % u9
                        local u13 = u7 * 798405
                        (function()
                            u12 = (u12 * u9 + u13) % 1099511627776
                            u8 = math.floor(u12 / u9)
                            u7 = u12 - u8 * u9
                        end)()
                        u10 = u10 + 1
                        debug.setupvalue(AC.attack, 5, u8)
                        debug.setupvalue(AC.attack, 6, u9)
                        debug.setupvalue(AC.attack, 4, u7)
                        debug.setupvalue(AC.attack, 7, u10)
                        pcall(function()
                            if plr.Character:FindFirstChildOfClass("Tool") and AC.blades and AC.blades[1] then
                                AC.animator.anims.basic[1]:Play(0.01,0.01,0.01)
                                game:GetService("ReplicatedStorage").RigControllerEvent:FireServer("weaponChange",tostring(GetCurrentBlade()))
                                game.ReplicatedStorage.Remotes.Validator:FireServer(math.floor(u12 / 1099511627776 * 16777215), u10)
                                game:GetService("ReplicatedStorage").RigControllerEvent:FireServer("hit", bladehit, i, "")
                            end
                        end)
                    end
                    end
    end

    pcall(function()
        while true do
            if Attacking then
                EquipWeapon(WeaponSelect)
                AttackNoCD()
            end
            wait()
        end
    end)
end)



